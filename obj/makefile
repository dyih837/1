-include ../makefile.defs

# Add inputs and outputs from these tool invocations to the build variables 
SECONDARY_FLASH += \
AWAVEN.hex \

SECONDARY_LIST += \
AWAVEN.lst \

SECONDARY_SIZE += \
AWAVEN.siz \


# 所有目标
all: AWAVEN.elf secondary-outputs

# 工具调用
AWAVEN.elf: $(OBJS) $(USER_OBJS)
	@	@	riscv-none-embed-gcc -march=rv32ecxw -mabi=ilp32e -msmall-data-limit=0 -msave-restore -Os -fmessage-length=0 -fsigned-char -ffunction-sections -fdata-sections -fno-common -Wunused -Wuninitialized  -g -T "C:\MRS_DATA\workspace\CH32V003J4M6\Ld\Link.ld" -nostartfiles -Xlinker --gc-sections -Wl,-Map,"AWAVEN.map" --specs=nano.specs --specs=nosys.specs -o "AWAVEN.elf" $(OBJS) $(USER_OBJS) $(LIBS) -lprintf
	@	@
AWAVEN.hex: AWAVEN.elf
	@	riscv-none-embed-objcopy -O ihex "AWAVEN.elf"  "AWAVEN.hex"
	@	@
AWAVEN.lst: AWAVEN.elf
	@	riscv-none-embed-objdump --all-headers --demangle --disassemble -M xw "AWAVEN.elf" > "AWAVEN.lst"
	@	@
AWAVEN.siz: AWAVEN.elf
	@	riscv-none-embed-size --format=berkeley "AWAVEN.elf"
	@	@
# 其他目标
clean:
	-$(RM) $(ASM_UPPER_DEPS)$(OBJS)$(SECONDARY_FLASH)$(SECONDARY_LIST)$(SECONDARY_SIZE)$(ASM_DEPS)$(S_DEPS)$(S_UPPER_DEPS)$(C_DEPS) AWAVEN.elf
	-@
secondary-outputs: $(SECONDARY_FLASH) $(SECONDARY_LIST) $(SECONDARY_SIZE)

.PHONY: all clean dependents

-include ../makefile.targets
